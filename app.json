{
  "expo": {
    "name": "treefy",
    "slug": "Treefy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-treefy-icon.png",
          "color": "#FCFDFF"
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FCFDFF"
    },
    "entryPoint": "./src/main/App.tsx",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.treefy.treefy",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.treefy.treefy",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FCFDFF"
      }
    },
    "web": {
      "favicon": "./assets/images/adaptive-icon.png"
    }
  }
}
